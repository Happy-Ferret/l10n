<bindings xmlns="http://www.mozilla.org/xbl" xmlns:xul="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul" xmlns:xbl="http://www.mozilla.org/xbl" xmlns:html="http://www.w3.org/1999/xhtml">
  <binding id="custom-browser-type-a">
    <content>
      <commandset>
        <command id="TypeA:Home" oncommand="home()"/>
        <command id="TypeA:Back" oncommand="back()"/>
      </commandset>
      <browser-toolbar cmd_home="TypeA:Home" cmd_back="TypeA:Back"/>
      <xul:browser/>
    </content>
  </binding>
  
  <binding id="browser-toolbar">
    <content>
      <xul:toolbar>
        <xul:toolbarbutton label="Home" xbl:inherits="command=cmd_home"/>
        <xul:toolbarbutton label="Back" xbl:inherits="command=cmd_back"/>
      </xul:toolbar>        
    </content>
  </binding>
  
  <binding id="my"> <!-- replaces contents of whatever this binding is applied to with this-->
    <content>
      <xul:button label="my button 1" xbl:inherits="command=cmd_home"/>
    </content>
  </binding>
</bindings>
